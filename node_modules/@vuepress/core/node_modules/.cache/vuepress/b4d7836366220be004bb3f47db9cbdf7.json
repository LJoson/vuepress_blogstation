<project-item :options=\"item\"></project-item>\n          </article>\n        </section>\n      </section>\n    </Common>\n  </div>\n</template>\n\n<script>\n  import Common from \"@theme/components/Common.vue\";\n  import { httpGet } from \"./utils/fetch.js\";\n  import ProjectItem from \"./ProjectItem\";\n  import { filter, orderBy } from \"lodash\";\n  export default {\n    name: \"Projects\",\n    components: {\n      Common,\n      ProjectItem\n    },\n    data() {\n      return {\n        projects: [],\n        loading: true\n      };\n    },\n    mounted() {\n      this.getProjects();\n    },\n    methods: {\n      getProjects() {\n        this.loading = true;\n        httpGet(\"https://api.github.com/users/LJoson/repos\").then(res => {\n          this.loading = false;\n          // 过滤掉私有的项目, 暂不过滤掉 fork 项目 && !item.fork\n          const projects = filter(res, item => !item.private);\n          this.projects = orderBy(\n            projects,\n            [\"stargazers_count\", \"forks_count\", \"name\", \"created_at\"],\n            [\"desc\", \"desc\", \"asc\", \"desc\"]\n          );\n        });\n      }\n    }\n  };\n</script>\n\n<style lang=\"css\" scoped>\n  .project {\n    max-width: 1024px;\n    min-height: 300px;\n    margin: 4rem auto;\n    margin-top: 6rem;\n  }\n  .project-title,\n  .project-subtitle {\n    text-align: center;\n  }\n  .project-loading {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .project-loading img {\n    position: relative;\n  }\n  .project-loading span {\n    position: absolute;\n  }\n  .project-container {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-around;\n  }\n</style>"]}]}